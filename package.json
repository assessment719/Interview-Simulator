{
    "name": "project",
    "version": "1.0.0",
    "scripts": {
        "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend-react\" \"npm run dev:server\"",
        "dev:frontend": "npm --prefix ./frontend run dev",
        "dev:backend-react": "npm --prefix ./backend run dev",
        "dev:server": "ts-node ./server/index.ts"
    },
    "dependencies": {
        "express": "^4.18.4"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "concurrently": "^8.2.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    }
}
